{
  "name": "nextjs15",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "sunlint": "sunlint --config=.sunlint.json --output-html=report.html --input=/src --github-annotate || true",
    "sunlint:changed": "sunlint --config=.sunlint.json --output-html=report.html --changed-files --github-annotate || true",
    "sunlint:security": "sunlint --security --input=src"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.0.0",
    "@hookform/resolvers": "^3.9.0",
    "@sun-asterisk/sunlint": "^1.3.41",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-query-devtools": "^5.62.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "antd": "^5.22.0",
    "axios": "^1.7.0",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.23",
    "next": "14.2.35",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.54.0",
    "zod": "^3.23.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.23",
    "@types/node": "^20.17.0",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "14.2.35",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.0",
    "prettier": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}